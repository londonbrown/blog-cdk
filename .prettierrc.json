{
  "semi": false,
  "trailingComma": "none",
  "singleQuote": false,
  "printWidth": 100,
  "plugins": ["prettier-plugin-organize-imports", "@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
